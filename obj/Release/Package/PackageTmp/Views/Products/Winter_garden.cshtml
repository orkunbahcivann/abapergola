                        <img loading="lazy" class="img-fluid" src="~/images/projects/wintergarden.jpeg" alt="project-image" />
                    </div>

                    <div class="item">
                        <img loading="lazy" class="img-fluid" src="~/images/projects/wintergarden2.jpeg" alt="project-image" />
                    </div>
                    <div class="item">
                        <img loading="lazy" class="img-fluid" src="~/images/projects/wintergarden3.jpeg" alt="project-image" />
                    </div>
                    @*<div class="item">
                        <img loading="lazy" class="img-fluid" src="~/images/pergola/pergola4.png" alt="project-image" />
                    </div>*@
